package uitest;

public class RecorderScript {

	
	ATUTestRecorder recorder = new ATUTestRecorder("Directory","filename",false);
	recorder.start(); 
	WebDriver browser = new FirefoxDriver();
	browser.navigate().to("https://opensource-demo.orangehrmlive.com/");
	browser.manage().window().maximize();  
	browser.quit();
	recorder.stop();
}
